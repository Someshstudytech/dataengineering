{
	"name": "008_ppl_jsoncopy",
	"properties": {
		"activities": [
			{
				"name": "Copy data json to sqltb",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['EmployeeKey']"
								},
								"sink": {
									"name": "EmployeeKey",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['FirstName']"
								},
								"sink": {
									"name": "FirstName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['LastName']"
								},
								"sink": {
									"name": "LastName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['EmailAddress']"
								},
								"sink": {
									"name": "EmailAddress",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['Phone']"
								},
								"sink": {
									"name": "Phone",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['city']"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['Country']"
								},
								"sink": {
									"name": "Country",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ds_json_source",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql_json_sink",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}